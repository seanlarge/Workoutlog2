<h1>Listing wods</h1>

<table>
  <thead>
    <tr>
      <th>Pr</th>
      <th>Score</th>
      <th>Benchmark</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wods.each do |wod| %>
      <tr>
        <td><%= wod.pr %></td>
        <td><%= wod.score %></td>
        <td><%= wod.benchmark %></td>
        <td><%= link_to 'Show', wod %></td>
        <td><%= link_to 'Edit', edit_wod_path(wod) %></td>
        <td><%= link_to 'Destroy', wod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wod', new_wod_path %>
